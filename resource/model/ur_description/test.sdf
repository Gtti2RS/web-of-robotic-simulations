<sdf version="1.10">
  <model name="test_joint">
    <link name="base"/>
    <link name="link1"/>
    <joint name="rev" type="revolute">
      <parent>base</parent>
      <child>link1</child>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
    </joint>
    <plugin name="gz::sim::systems::JointPositionController" filename="gz-sim-joint-position-controller-system">
      <joint_name>rev</joint_name>
      <p_gain>100</p_gain>
      <d_gain>5</d_gain>
    </plugin>
  </model>
</sdf>
